# Example environment overrides for Confluence
# Copy to config/.env and edit as needed

CONFLUENCE_LOG_LEVEL=INFO

# Provider API keys
CONFLUENCE_PROVIDER_KEYS__ALPHAVANTAGE_KEY=
CONFLUENCE_PROVIDER_KEYS__POLYGON_KEY=
CONFLUENCE_PROVIDER_KEYS__FINNHUB_KEY=

# Exchange keys (use OS keyring preferred)
CONFLUENCE_EXCHANGE_KEYS__BINANCE_KEY=
CONFLUENCE_EXCHANGE_KEYS__BINANCE_SECRET=
CONFLUENCE_EXCHANGE_KEYS__BINANCEUS_KEY=
CONFLUENCE_EXCHANGE_KEYS__BINANCEUS_SECRET=
CONFLUENCE_EXCHANGE_KEYS__ROBINHOOD_CRYPTO_KEY=
CONFLUENCE_EXCHANGE_KEYS__ROBINHOOD_CRYPTO_SECRET=

# Alerts
CONFLUENCE_ALERTS__SLACK_WEBHOOK_URL=
CONFLUENCE_ALERTS__TELEGRAM_BOT_TOKEN=
CONFLUENCE_ALERTS__TELEGRAM_CHAT_ID=
CONFLUENCE_ALERTS__SMTP_HOST=
CONFLUENCE_ALERTS__SMTP_PORT=587
CONFLUENCE_ALERTS__SMTP_USER=
CONFLUENCE_ALERTS__SMTP_FROM=
CONFLUENCE_ALERTS__SMTP_TLS=true

# DB
CONFLUENCE_DATABASE_URL=sqlite+aiosqlite:///./confluence.db

